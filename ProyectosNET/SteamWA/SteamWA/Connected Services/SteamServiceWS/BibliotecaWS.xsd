      <xs:element minOccurs="0" name="razonSocial" type="xs:string" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="asignarBibliotecaUsuario">
    <xs:sequence>
      <xs:element name="uid_usuario" type="xs:int" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="asignarBibliotecaUsuarioResponse">
    <xs:sequence>
      <xs:element name="return" type="xs:int" />
    </xs:sequence>
  </xs:complexType>
</xs:schema>